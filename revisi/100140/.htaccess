<IfModule mod_rewrite.c>
  RewriteEngine on 
  RewriteCond %{REQUEST_FILENAME} !-d 
  RewriteCond %{REQUEST_FILENAME}.php -f 
  RewriteRule ^(.*)$ $1.php
</IfModule>
<IfModule mod_rewrite.c>
  RewriteEngine on 
  RewriteCond %{REQUEST_FILENAME} !-d 
  RewriteCond %{REQUEST_FILENAME}.html -f 
  RewriteRule ^(.*)$ $1.html
</IfModule>
ErrorDocument 404 http://localhost/100140/
IndexIgnore *.gif *.zip *.txt *.png *.php *.mp4
